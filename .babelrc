{
  "env": {
    "test": {
      "presets": [
        "@babel/preset-react",
        [
          "@babel/preset-env",
          {
            "shippedProposals": true,
            "modules": "commonjs"
          }
        ],
        [
          "@babel/preset-stage-2",
          {
            "decoratorsLegacy": true
          }
        ]
      ],
      "plugins": [
        "dynamic-import-node-babel-7"
      ]
    }
  }
}
