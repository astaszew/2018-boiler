{
  "compilerOptions": {
    "sourceMap": true,
    "module": "esnext",
    "target": "esnext",
    "jsx": "react-native",
    "noImplicitAny": false,
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "listEmittedFiles": true,
    "moduleResolution": "node",
    "lib": [ "es2015", "dom" ],
  },
  "compileOnSave": true,
  "include": [
    "src/**/*",
    "types/**/*"
  ],
  "typeRoots": [
    "node_modules/@types",
    "types"
  ],
  "exclude": [
    "node_modules",
    "**/*.js"
  ]
}
