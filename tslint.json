{
  "extends": "tslint-config-airbnb",
  "rules": {
    "indent": [true, "spaces", 2],
    "ter-indent": [true, 2],
    "max-line-length": [false],
    "radix": false,
    "no-increment-decrement": false,
    "space-before-function-paren": false,
    "import-name": false,
    "variable-name": false,
    "object-curly-spacing": [
      true,
      "never"
    ]
  }
}
